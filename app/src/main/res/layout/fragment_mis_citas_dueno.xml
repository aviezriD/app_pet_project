<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">
<data>
    <variable name="dateViewModel" type="com.mx.mascotas.presentation.ui.main.vet.date.DateViewModel"/>
</data>
<FrameLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:id="@+id/dateListDatesOwner"/>
    <TextView
            android:id="@+id/cero"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="16dp"
            android:text="@string/date_welcome_owner"
            android:textColor="@color/colorAccent"
            android:textSize="24sp"
            android:textStyle="bold" android:layout_gravity="center"/>
</FrameLayout>
</layout>
